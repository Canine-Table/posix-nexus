link add bridge0 type bridge \
mcast_igmp_version 3 \
vlan_stats_per_port 1 \
vlan_protocol "802.1Q" \
vlan_default_pvid 999 \
vlan_filtering 1 \
mdb_offload_fail_notification 1 \
