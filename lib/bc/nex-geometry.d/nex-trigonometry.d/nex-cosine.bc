
##################################################################
define r_nx_ts_cos(x) {
	return nx_ts(nx_pt_mod2pi(x), 2, 2, 1)
}

##################################################################
define r_nx_ts_sec(x) {
	return 1 / r_nx_ts_cos(x)
}

##################################################################
define r_nx_ts_acos(x) {
	if (x > 1 || x < -1) {
		print "<nx:impurity230/>"
		return -1
	}
	return c_pi_2 - r_nx_ts_asin(x)
}

